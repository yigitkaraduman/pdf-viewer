@model SvgAccessModel

      <div class="modal" id="accessKeyModal">
          <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content">
              <div class="modal-header">
                <h5 class="modal-title">Configurations</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close">
                  <span aria-hidden="true"></span>
                </button>
              </div>

                @{
                     var data = ViewBag.msg;
                }

              <form asp-action="Access" method="post">
                  <div class="modal-body">        
                      <div class="mb-3">
                        
                        <label asp-for="PrintSvcUrl"></label>
                        <input asp-for="PrintSvcUrl" class="form-control" value="@data.PrintSvcUrl"/>
                        <span asp-validation-for="PrintSvcUrl" class="text-danger"></span>
                                          
                      </div>
                      <hr>
                      <div class="mb-3">
                        <label asp-for="AuthSvcUrl"></label>
                        <input asp-for="AuthSvcUrl" class="form-control" value="@data.AuthSvcUrl"/>
                        <span asp-validation-for="AuthSvcUrl" class="text-danger"></span>
                      
                      </div>
                       <div class="mb-3">
                        <label asp-for="CevreSistem"></label>
                        <input asp-for="CevreSistem" class="form-control" value="@data.CevreSistem"/>
                        <span asp-validation-for="CevreSistem" class="text-danger"></span>
                     
                      </div>
                       <div class="mb-3">
                         <label asp-for="SirketID"></label>
                        <input asp-for="SirketID" class="form-control" value="@data.SirketID"/>
                        <span asp-validation-for="SirketID" class="text-danger"></span>
                     
                      </div>
                       <div class="mb-3">
                         <label asp-for="User"></label>
                        <input asp-for="User" class="form-control" value="@data.User"/>
                        <span asp-validation-for="User" class="text-danger"></span>
                  
                      </div>
                       <div class="mb-3">
                         <label asp-for="Password"></label>
                        <input type="password" asp-for="Password" class="form-control" value="@data.Password"/>
                        <span asp-validation-for="Password" class="text-danger"></span>
             
                      </div>        
                  </div>
                  <div class="modal-footer">
                    <button type="submit" class="btn btn-primary">Save changes</button>
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                  </div>
              </form>
            </div>
          </div>
        </div>